target 'Prajna' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Prajna
  # 响应式编程框架
  pod 'RxSwift', '6.8.0'
  # RxSwift在UIKit框架上的扩展
  pod 'RxCocoa', '6.8.0'
  # 用于简化Auto Layout约束的DSL
  pod 'SnapKit', '5.7.1'
  # 网络请求库
  pod 'Alamofire', '5.9.0'
  # 下拉刷新和上拉加载更多控件
  pod 'MJRefresh', '3.7.7'
  # 图片下载和缓存库
  pod 'Kingfisher', '7.11.0'
  # JSON序列化/反序列化库
  pod 'HandyJSON', '5.0.2'
  # 简单的提示框库
  pod 'Toast-Swift', '5.1.0'
  # 基于Alamofire的网络抽象层，支持RxSwift
  pod 'Moya/RxSwift', '15.0.0'
  # 类似于iOS13的模态视图控制器
  pod 'HWPanModal', '0.9.8'
  # RSA加密解密库
  pod 'SwiftyRSA', '1.7.0'
  # Swift加密库，提供多种加密算法
  pod 'CryptoSwift', '1.8.2'
  # 加载指示器集合
  pod 'NVActivityIndicatorView', '5.1.1'
  # 腾讯开源的UI组件库
  pod 'QMUIKit'
  pod 'SwiftyUserDefaults', '5.3.0'
  
  pod 'CocoaLumberjack/Swift', '3.8.5'

  # Gemini, 通过firebase ai安装
  #  pod 'FirebaseAuth'
  #  pod 'FirebaseFirestore'
    pod 'FirebaseAI'
    
  target 'PrajnaTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'PrajnaUITests' do
    # Pods for testing
  end

